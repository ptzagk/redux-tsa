{
  "name": "redux-tsa",
  "version": "0.0.0",
  "description": "Async friendly validation middleware for Redux",
  "main": "src",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --fix --type-check -c tslint.json --project tsconfig.json",
    "test": "jest",
    "testcov": "jest --coverage",
    "postcov": "codecov -f coverage/*.json",
    "sandbox": "jest asyncProcess.test.ts"
  },
  "author": "Mateusz Okon <teuszokon@gmail.com> (http://mateuszokon.com/)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "awesome-typescript-loader": "^3.1.3",
    "codecov": "^2.2.0",
    "jest": "^20.0.4",
    "redux": "^3.6.0",
    "source-map-loader": "^0.2.1",
    "ts-jest": "19.02",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1"
  },
  "peerDependencies": {
    "redux": "^3.6.0"
  },
  "jest": {
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "example"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ]
  },
  "dependencies": {}
}
